<ul>
  <% ["just", "some", "words"].each do |word| %>
    <li><%= word %></li>
  <% end %>
</ul>

<ol>
  <% 5.times do |counter| %>
    <li>say <%= counter + 1 %>!</li>
  <% end %>
</ol>

<% name = "Muneca" %>

<p>
  <% if name == "Muneca" %>
    <%= "Hi, #{name}!" %>
  <% else %>
    Oi!
  <% end %>
</p>
